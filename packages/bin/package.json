{
  "name": "@ffmpeg-oneclick/bin",
  "version": "0.1.4",
  "description": "FFmpeg binary downloader and manager for ffmpeg-oneclick",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "binaries",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "ffmpeg",
    "binary",
    "downloader"
  ],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "extract-zip": "^2.0.1",
    "https-proxy-agent": "^7.0.2",
    "node-fetch": "^3.3.2",
    "tar": "^7.5.9"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/tar": "^6.1.10",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
